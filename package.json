    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "framer-motion": "^10.10.0",
    "immer": "^9.0.21",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "next": "^13.2.4",
    "next-themes": "^0.2.1",
    "popmotion": "^11.0.5",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "recoil": "^0.7.7",
