{
  "name": "nextjs-template",
  "version": "1.0.0",
  "license": "GPL-2.0",
  "scripts": {
    "prepare": "husky install",
    "dev": "concurrently \"cross-env NODE_ENV=development node server.js\"",
    "dev:secure": "concurrently \"cross-env NODE_ENV=production ENABLE_HTTPS=true node server.js\" \"yarn test:dev\"",
    "build": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:dev": "jest --watch",
    "test:build": "jest",
    "lint": "eslint --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ],
    "src/**": [
      "prettier --write ."
    ]
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.2.4",
    "axios": "^1.3.4",
    "compression": "^1.7.4",
